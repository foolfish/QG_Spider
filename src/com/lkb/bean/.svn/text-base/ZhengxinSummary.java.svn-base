package com.lkb.bean;

public class ZhengxinSummary {
	private String id;
	private String loginName;      
	private String state;            //账户状态
	private int xinyNum;			//信用卡账户数
	private int zhufNum;			//住房贷款账户数
	private int qitNum;				//其他账户数
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}

	public String getLoginName() {
		return loginName;
	}
	public void setLoginName(String loginName) {
		this.loginName = loginName;
	}
	public String getState() {
		return state;
	}
	public void setState(String state) {
		this.state = state;
	}
	public int getXinyNum() {
		return xinyNum;
	}
	public void setXinyNum(int xinyNum) {
		this.xinyNum = xinyNum;
	}
	public int getZhufNum() {
		return zhufNum;
	}
	public void setZhufNum(int zhufNum) {
		this.zhufNum = zhufNum;
	}
	public int getQitNum() {
		return qitNum;
	}
	public void setQitNum(int qitNum) {
		this.qitNum = qitNum;
	}
	
}
