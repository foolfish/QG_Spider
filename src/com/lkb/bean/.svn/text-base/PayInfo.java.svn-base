package com.lkb.bean;

import java.math.BigDecimal;
import java.util.Date;


public class PayInfo {
	private String id; 
	private Date payTime; //支付时间
	private String tradeType;//交易类型  余额宝-单次转入   	付款-转账  代付  火车票   	关注地贫患儿 
	private String tradeNoType;//交易账号类型：流水号 或者 交易号	
	private String tradeNo;//交易号 2014031511001004800040082850
	private String receiverName;//接收人	
	private BigDecimal amount;//交易金额 	
	private String status;//交易状态 
	private String source;//交易来源
	private String alipayName;//支付宝名称

	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public Date getPayTime() {
		return payTime;
	}
	public void setPayTime(Date payTime) {
		this.payTime = payTime;
	}
	public String getTradeType() {
		return tradeType;
	}
	public void setTradeType(String tradeType) {
		this.tradeType = tradeType;
	}
	public String getTradeNoType() {
		return tradeNoType;
	}
	public void setTradeNoType(String tradeNoType) {
		this.tradeNoType = tradeNoType;
	}
	public String getTradeNo() {
		return tradeNo;
	}
	public void setTradeNo(String tradeNo) {
		this.tradeNo = tradeNo;
	}
	public String getReceiverName() {
		return receiverName;
	}
	public void setReceiverName(String receiverName) {
		this.receiverName = receiverName;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public String getSource() {
		return source;
	}
	public void setSource(String source) {
		this.source = source;
	}
	public BigDecimal getAmount() {
		return amount;
	}
	public void setAmount(BigDecimal amount) {
		this.amount = amount;
	}
	public String getAlipayName() {
		return alipayName;
	}
	public void setAlipayName(String alipayName) {
		this.alipayName = alipayName;
	}


	
}
